{
  "trailingComma": "all",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "insertPragma": false,
  "bracketSameLine": false,
  "printWidth": 80,
  "proseWrap": "always",
  "requirePragma": false,
  "useTabs": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
